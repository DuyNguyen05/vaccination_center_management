= form_with model: [:admin, @company, @order] do |f|
  = f.hidden_field :account_id, value: current_admin_admin.id
  = f.hidden_field :company_id, value: @company.id
  .form-group
    = f.label Order.human_attribute_name(:vaccine_id)
    = f.select :vaccine_id, options_for_select(@vaccines), {:include_hidden => false}, class: "form-control", id: "vaccineCompany", multiple: true
  .form-group
    = f.label Order.human_attribute_name(:quantity)
    = f.text_field "quantity[]", class: "form-control"
  = f.submit t(".submit"), class: "btn btn-primary"
