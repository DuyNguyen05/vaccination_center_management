.modal.fade.bd-example-modal-lg{"aria-hidden" => "true", "aria-labelledby" => "myLargeModalLabel", :role => "dialog"  , id: "modalDetailVaccinePackageEdit#{detail_vaccine_package.id}"}
  .modal-dialog.modal-lg
    .modal-content
      .modal-header
        %h4
          %b= t(".edit_detail_vaccine_package")
        %button.close{"aria-label" => "Close", "data-dismiss" => "modal", :type => "button"}
          %span{"aria-hidden" => "true"} Ã—
      .modal-body
        = form_with model: [:admin, detail_vaccine_package], method: :patch do |f|
          .form-group
            = f.label DetailVaccinePackage.human_attribute_name(:vaccine_id)
            = f.select :vaccine_id, options_for_select(vaccines), {},class: "form-control", id: "detail-vaccine-package"
          .form-group
            = f.label DetailVaccinePackage.human_attribute_name(:total_injections)
            = f.number_field :total_injections, class: "form-control"
          = f.submit t(".submit"), class: "btn btn-primary"
