<% if @vaccine_package_type.errors.any? %>
  toastr["warning"]("<%= I18n.t("error") %> <%= j @vaccine_package_type.errors.full_messages.join('; ') %>")
<% else %>
  $("#vaccine_package_types").html("<%= j render partial: "vaccine_package_type", collection: @vaccine_package_types %>");
  $("#modalVaccinePackageTypeEdit<%= @vaccine_package_type.id %>").modal("hide");
  toastr["info"]("<%= I18n.t("admin.vaccine_package_types.update.updated") %>");
<% end %>
