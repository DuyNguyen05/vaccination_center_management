<% if @vaccine_package_type.errors.any? %>
  alert("<%= I18n.t("error") %> <%= j @vaccine_package_type.errors.full_messages.join('; ') %>")
<% else %>
  $("#vaccine_package_types").html("<%= j render partial: "vaccine_package_type", collection: @vaccine_package_types %>");
  $("#modalVaccinePackageType").modal("hide");
  $(".modal-body input").val("")
  toastr["info"]("<%= I18n.t("admin.vaccine_package_types.create.created") %>");
<% end %>
