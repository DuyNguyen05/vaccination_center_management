<% if @company.errors.any? %>
  toastr["warning"]("<%= I18n.t("error") %> <%= j @company.errors.full_messages.join('; ') %>")
<% else %>
  $("#companies").html("<%= j render 'admin/companies/company', companies: @companies %>");
  $("#modalCompany").modal("hide");
  $(".modal-body input").val("")
<% end %>
